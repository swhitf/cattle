{
  "name": "cattle",
  "version": "1.4.1",
  "description": "A spreadsheet-like grid component library.",
  "main": "dist/lib/index.js",
  "typings": "dist/lib/index.d.ts",
  "scripts": {
    "build": "node scripts/build.js",
    "clean": "node scripts/build.js --task=clean",
    "export": "node scripts/build.js --task=clean && node scripts/index.js && tsc --pretty",
    "start": "concurrently -k \"node scripts/build.js -w \" \"http-server ./dist/dev -s -c-1\"",
    "prepublish": "node scripts/build.js --task=clean && node scripts/index.js && tsc --pretty"
  },
  "author": "Stephen Whitfield",
  "license": "MIT",
  "dependencies": {
    "@types/bases": "^0.2.2",
    "@types/papaparse": "^4.1.33",
    "bases": "^0.2.1",
    "chrono-node": "^1.3.5",
    "moment": "^2.21.0",
    "papaparse": "^4.3.7",
    "parse-color": "^1.0.0",
    "reflect-metadata": "^0.1.12",
    "resize-observer-polyfill": "^1.5.0",
    "tiny-sha256": "^1.0.2"
  },
  "devDependencies": {
    "browserify": "^14.5.0",
    "es5-shim": "^4.5.10",
    "es6-shim": "^0.35.3",
    "fs-extra": "^5.0.0",
    "gaze": "^1.1.2",
    "optimist": "^0.6.1",
    "tsify": "^3.0.4",
    "typescript": "^2.6.2",
    "watchify": "^3.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:swhitf/cattle.git"
  },
  "keywords": [
    "spreadsheet",
    "grid",
    "datagrid",
    "sheets",
    "excel"
  ]
}
